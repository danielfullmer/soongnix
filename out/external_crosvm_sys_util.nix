{ rust_library_host_rlib }:
let

#  This file is generated by cargo2android.py, manually added "libcap".

libsys_util = rust_library_host_rlib {
    name = "libsys_util";
    defaults = ["crosvm_defaults"];
    deny_warnings = false;
    crate_name = "sys_util";
    srcs = ["src/lib.rs"];
    rlibs = [
        "libdata_model"
        "liblibc"
        "libsync_rust"
        "libsyscall_defines"
    ];
    proc_macros = [
        "libpoll_token_derive"
    ];
    shared_libs = ["libcap"]; #  specified in src/capabilities.rs
};

in { inherit libsys_util; }
